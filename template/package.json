{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appIdSuffix staging",
    "androidStaging": "react-native run-android --variant=stagingRelease --appIdSuffix staging",
    "androidRelease": "react-native run-android --variant=release",
    "ios": "ENVFILE=.env.staging react-native run-ios",
    "iosRelease": "ENVFILE=.env.production react-native run-ios --configuration=Release",
    "iosStaging": "ENVFILE=.env.staging react-native run-ios --configuration=Staging",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pretty": "prettier --check 'src/**/*.{ts,tsx}' --write",
    "tsCheck": "tsc -p ./",
    "postinstall": "patch-package",
    "makeIcons": "npx @svgr/cli --ignore-existing -d src/assets/icons icons && yarn pretty",
    "patch": "node versionBump",
    "minor": "node versionBump version=minor",
    "major": "node versionBump version=major",
    "codepush": "node codepush"
  },
  "dependencies": {
    "@bugsnag/plugin-react-navigation": "7.10.0",
    "@bugsnag/react-native": "7.10.1",
    "@react-native-async-storage/async-storage": "1.15.5",
    "@react-navigation/bottom-tabs": "6.0.0-next.17",
    "@react-navigation/native": "6.0.0-next.8",
    "axios": "0.21.1",
    "immer": "9.0.2",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-bootsplash": "3.2.3",
    "react-native-code-push": "7.0.1",
    "react-native-config": "1.4.2",
    "react-native-device-info": "^8.1.3",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "1.10.3",
    "react-native-inappbrowser-reborn": "^3.5.1",
    "react-native-reanimated": "2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "3.3.0",
    "react-query": "3.16.0",
    "tinycolor2": "^1.4.2",
    "zustand": "3.5.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-transform-react-jsx": "^7.14.3",
    "@babel/runtime": "7.12.5",
    "@bugsnag/source-maps": "2.1.0",
    "@lightbase/eslint-config-lightbase": "2.14.0",
    "@react-native-community/eslint-config": "2.0.0",
    "@testing-library/jest-native": "4.0.1",
    "@testing-library/react-native": "7.2.0",
    "@types/jest": "26.0.23",
    "@types/react-native": "0.64.8",
    "@types/react-test-renderer": "16.9.2",
    "@types/tinycolor2": "^1.4.2",
    "babel-jest": "27.0.2",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "7.27.0",
    "flipper-plugin-bridgespy-client": "0.1.9",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.66.0",
    "patch-package": "6.4.7",
    "promise-polyfill": "8.2.0",
    "react-native-flipper": "0.91.2",
    "react-test-renderer": "17.0.1",
    "reactotron-react-native": "5.0.0",
    "rn-async-storage-flipper": "0.0.10",
    "typescript": "4.3.2",
    "xmlhttprequest": "1.8.0"
  }
}
